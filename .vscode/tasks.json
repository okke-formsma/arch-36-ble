{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "flash master,
			"command": "/usr/bin/make",
			"args": [
				"flash_master",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "flash slave,
			"command": "/usr/bin/make",
			"args": [
				"flash_slave",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}